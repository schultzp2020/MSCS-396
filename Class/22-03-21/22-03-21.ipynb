   "source": [
    "def fermat(n):\n",
    "  x = math.isqrt(n)\n",
    "  i = 0\n",
    "  if x**2 == n:\n",
    "    return x, x, i\n",
    "  x += 1\n",
    "  while math.isqrt(x**2 - n)**2 != (x**2 - n):\n",
    "    x += 1\n",
    "    i += 1\n",
    "  y = math.isqrt(x**2 - n)\n",
    "  return x+y, x-y, i"
   ]
